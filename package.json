  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./lib/index.esm.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.esm.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "source": "./src/index.ts",
